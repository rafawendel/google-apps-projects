{
  "files.autoSave": "onFocusChange",
  "git.enabled": true,
  "prettier.singleQuote": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 4,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": ["javascript"]
}
